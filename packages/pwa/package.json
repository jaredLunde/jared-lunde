{
  "name": "pwa",
  "version": "1.0.0",
  "main": "src/index.js",
  "license": "MIT",
  "devDependencies": {
    "@inst-app/serverless-pwa": "^1.0.4",
    "terser-webpack-plugin": "^1.1.0"
  },
  "dependencies": {
    "@inst-app/static-pwa": "^1.0.3",
    "@jaredlunde/react-with-sticky": "^1.0.4",
    "@render-props/hover": "^0.1.10",
    "@render-props/paragraphs": "^0.1.9",
    "@render-props/rect": "^0.1.16",
    "@render-props/size-observer": "^0.1.12",
    "express": "^4.16.3",
    "react-portalize": "^0.1.9",
    "react-router-transition": "^1.2.1",
    "serverless-plugin-lambda-warmup": "^1.0.1"
  },
  "inst": {
    "type": "pwa",
    "awsProfile": {
      "staging": "instaspace-dev",
      "production": "instaspace-prod"
    },
    "s3": {
      "staging": "lunde-staging-public",
      "production": "lunde-public"
    }
  },
  "sideEffects": false,
  "scripts": {
    "analyze": "webpack-bundle-analyzer dist/staging/client/full-stats.json",
    "analyze:prod": "webpack-bundle-analyzer dist/production/client/full-stats.json",
    "edit": "atom ./",
    "start": "inst start --ssr",
    "deploy": "inst deploy staging",
    "deploy:prod": "inst deploy production",
    "deploy-client": "inst deploy-client staging",
    "deploy-client:prod": "inst deploy-client production",
    "deploy-server": "inst deploy-server staging",
    "deploy-server:prod": "inst deploy-server production"
  }
}
